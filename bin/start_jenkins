#!/bin/sh

docker run -it --rm --name jenkins01 -d \
                --expose 8443 \
                --expose 8080 \
                --env JENKINS_PASSWORD=0000 \
                --env OPENSHIFT_ENABLE_OAUTH=false \
               openshift/jenkins-2-centos7 

docker logs -f jenkins01

